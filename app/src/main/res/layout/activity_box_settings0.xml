<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BoxSettings0"
    android:background="#E0DCD7"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="0dp">

    <ImageView
        android:id="@+id/btn_boxsettings_back"
        android:layout_width="29dp"
        android:layout_height="29dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/arrow_back" />

    <TextView
        android:id="@+id/label_box_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Box settings"
        android:textColor="#121212"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.233"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <TextView
        android:id="@+id/label_info_sim_card"
        android:layout_width="312dp"
        android:layout_height="26dp"
        android:text="Set or change the box's sim card number"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_boxsettings_sim_card"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/label_info_user"
        android:layout_width="312dp"
        android:layout_height="30dp"
        android:text="Introduce your account to the box"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_boxsettings_user"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/label_boxsettings_user"
        android:layout_width="312dp"
        android:layout_height="33dp"
        android:text="User"
        android:textColor="#121212"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_3"
        app:layout_constraintVertical_bias="0.018" />

    <ImageView
        android:id="@+id/img_user"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/divider_5"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_3"
        app:layout_constraintVertical_bias="0.025"
        app:srcCompat="@drawable/person" />

    <View
        android:id="@+id/divider_4"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_info_user"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/label_boxsettings_sim_card"
        android:layout_width="312dp"
        android:layout_height="30dp"
        android:text="Sim card"
        android:textColor="#121212"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_4"
        app:layout_constraintVertical_bias="0.023" />

    <View
        android:id="@+id/divider_5"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_info_sim_card"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/img_pin"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_5"
        app:layout_constraintVertical_bias="0.027"
        app:srcCompat="@drawable/pin" />


    <TextView
        android:id="@+id/label_boxsettings_pin_code"
        android:layout_width="312dp"
        android:layout_height="33dp"
        android:text="Pin code"
        android:textColor="#121212"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_5"
        app:layout_constraintVertical_bias="0.029" />

    <TextView
        android:id="@+id/label_info_pin_code"
        android:layout_width="312dp"
        android:layout_height="30dp"
        android:text="Change the box's pin code"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_boxsettings_pin_code"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/divider_6"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_info_pin_code"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/img_sim"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_4"
        app:layout_constraintVertical_bias="0.027"
        app:srcCompat="@drawable/sim_card" />

    <TextView
        android:id="@+id/label_boxsettings_wifi_connection"
        android:layout_width="312dp"
        android:layout_height="33dp"
        android:layout_marginTop="11dp"
        android:text="Wi-Fi connection"
        android:textColor="#121212"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/divider_6"
        app:layout_constraintHorizontal_bias="0.917"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/label_info_wifi_connection"
        android:layout_width="312dp"
        android:layout_height="30dp"
        android:text="Set or change the box's Wi-Fi connection"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_boxsettings_wifi_connection"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/label_info"
        android:layout_width="392dp"
        android:layout_height="48dp"
        android:text="See box information like the Wi-Fi and user it's connected to, sim card number, and pin number."
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_1"
        app:layout_constraintVertical_bias="0.012" />


    <View
        android:id="@+id/divider_1"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_box_settings"
        app:layout_constraintVertical_bias="0.022" />

    <View
        android:id="@+id/divider_2"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_info"
        app:layout_constraintVertical_bias="0.014" />

    <View
        android:id="@+id/divider_3"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label_info_wifi_connection"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/img_wifi"
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_2"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/wifi" />


</androidx.constraintlayout.widget.ConstraintLayout>